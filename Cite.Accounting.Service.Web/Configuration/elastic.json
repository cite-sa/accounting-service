{
	"ElasticCertificateConfig": {
		"LoadAdditionalSslCertificates": "%{ElasticLoadAdditionalSslCertificates}%",
		"Paths": [ "%{ElasticCertificatePath}%" ]
	},
	"ElasticConfig": {
		"EnableDebugMode": false,
		"PrettyJson": false,
		"ConnectionLimit": 80,
		"DeadTimeOut": null,
		"DisableAuditTrail": true,
		"DisableAutomaticProxyDetection": true,
		"DisableDirectStreaming": true,
		"DisableMetaHeader": true,
		"DisablePing": true,
		"DnsRefreshTimeout": null,
		"EnableHttpCompression": true,
		"EnableTcpKeepAliveTime": null,
		"EnableTcpKeepAliveInterval": null,
		"EnableHttpPipelining": true,
		"MaxDeadTimeout": null,
		"MaximumRetries": 3,
		"MaxRetryTimeout": null,
		"PingTimeout": null,
		"RequestTimeout": null,
		"SniffLifeSpan": null,
		"SniffOnConnectionFault": true,
		"SniffOnStartup": true,
		"UserName": "%{ElasticUserName}%",
		"Password": "%{ElasticPassword}%",
		"AuthenticationType": "Basic",
		"ConnectionType": 0, //Single = 0, Cloud = 1, Static = 2, Sniffing = 3, Sticky = 4
		"SingleNodeConnection": {
			"Uri": "%{ElasticUri}%"
		},
		"AccountingEntryIndex": {
			"Name": "%{ElasticAccountingEntryIndex}%",
			"MaxResultWindow": 10000
		},
		"UserInfoIndex": {
			"Name": "%{ElasticUserInfoIndex}%",
			"MaxResultWindow": 10000
		},
		"DefaultResultSize": 100,
		"DefaultScrollSize": 100,
		"DefaultCollectAllResultSize": 1000,
		"DefaultScrollSeconds": 120,
		"DefaultCompositeAggregationResultSize": 100,
		"Logging": {
			"EnableRequestLogging": false,
			"EnableResponseLogging": false
		},
		"ElasticClientType": "%{ElasticClientType}%"
	}
}
