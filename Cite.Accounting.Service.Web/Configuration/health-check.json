			},
			"Live": {
				"IsEnabled": true,
				"RequireHosts": null,
				"EndpointPath": "/health-check/live",
				"IncludeTag": "live",
				"AllowCaching": false,
				"HealthyStatusCode": 200,
				"DegradedStatusCode": 200,
				"UnhealthyStatusCode": 503,
				"VerboseResponse": true
			}
		}
	}
}
